                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         \n      </td>\n    </tr>\n  </tbody>\n</table>\n\n  \n  <h6 class=\"no-anchor\" id=\"返回值\">返回值</h6>\n  <p><code>Observable&lt;<a href=\"api/common/http/HttpEvent\" class=\"code-anchor\">HttpEvent</a>&lt;any&gt;&gt;</code></p>\n\n  \n\n\n  \n\n  \n</div>\n      </td>\n    </tr>\n  \n\n  \n\n  \n    </tbody>\n</table>\n\n  \n</section>\n\n\n\n  \n\n\n  </div>\n</article>\n\n<!-- links to this doc:\n - api/common/http\n - api/common/http/HttpClientModule\n - api/common/http/HttpHandler\n - api/common/http/HttpXhrBackend\n - api/common/http/JsonpClientBackend\n - api/common/http/testing\n - api/common/http/testing/HttpClientTestingModule\n - guide/http\n - guide/ngmodule-faq\n-->\n<!-- links from this doc:\n - /api\n - api/common\n - api/common/http\n - api/common/http/HttpBackend#description\n - api/common/http/HttpBackend#handle\n - api/common/http/HttpBackend#httpbackend\n - api/common/http/HttpBackend#instance-methods\n - api/common/http/HttpBackend#provided-in\n - api/common/http/HttpBackend#子類別\n - api/common/http/HttpClient\n - api/common/http/HttpClientModule\n - api/common/http/HttpEvent\n - api/common/http/HttpHandler\n - api/common/http/HttpRequest\n - api/common/http/HttpXhrBackend\n - api/common/http/JsonpClientBackend\n - api/common/http/testing/HttpClientTestingModule\n - https://github.com/angular/angular-cn/edit/aio/packages/common/http/src/backend.ts?message=docs(common)%3A%20請簡述你的修改...#L35-L54\n - https://github.com/angular/angular-cn/edit/aio/packages/common/http/src/backend.ts?message=docs(common)%3A%20請簡述你的修改...#L53-L53\n - https://github.com/angular/angular-cn/tree/12.1.0-next.1/packages/common/http/src/backend.ts#L35-L54\n - https://github.com/angular/angular-cn/tree/12.1.0-next.1/packages/common/http/src/backend.ts#L53-L53\n-->"}