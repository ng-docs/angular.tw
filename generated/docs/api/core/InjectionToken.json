                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 \n      </nt-wrapper></td>\n    </tr>\n    \n  </tbody>\n</table>\n\n  \n\n\n  \n\n  \n</div>\n      </td>\n    </tr>\n  \n\n  \n\n  \n    </tbody>\n</table>\n\n\n\n\n<section ng-should-translate=\"\" class=\"instance-properties\">\n  <a name=\"properties\"></a>\n  <h2 id=\"instance-properties\">屬性<a title=\"Link to this heading\" class=\"header-link\" aria-hidden=\"true\" href=\"api/core/InjectionToken#instance-properties\"><i class=\"material-icons\">link</i></a></h2>\n  <table class=\"is-full-width list-table property-table\">\n    <thead>\n      <tr>\n        <th>屬性</th>\n        <th>說明</th>\n      </tr>\n    </thead>\n    <tbody>\n    \n      <tr class=\"instance-properties\">\n        <td>\n          <a id=\"_desc\"></a>\n          <code class=\"\">protected <span class=\"member-name\">_desc</span>: string</code>\n        </td>\n        <td>\n           <span class=\"from-constructor\">宣告在建構函式中</span>\n          \n          \n          </td>\n    </tr>\n    \n    </tbody>\n  </table>\n</section>\n\n\n\n<section ng-should-translate=\"\" class=\"instance-methods\">\n  <a name=\"methods\"></a>\n  <h2 id=\"instance-methods\">方法<a title=\"Link to this heading\" class=\"header-link\" aria-hidden=\"true\" href=\"api/core/InjectionToken#instance-methods\"><i class=\"material-icons\">link</i></a></h2>\n  \n    <a id=\"toString\"></a>\n<table class=\"is-full-width method-table instance-methods\">\n  <thead><tr><th>\n    <div class=\"with-github-links\">\n    <h3 id=\"tostring\">\n      toString()\n      \n    <a title=\"Link to this heading\" class=\"header-link\" aria-hidden=\"true\" href=\"api/core/InjectionToken#tostring\"><i class=\"material-icons\">link</i></a></h3>\n    <div class=\"github-links\">\n  <a href=\"https://github.com/angular/angular-cn/edit/aio/packages/core/src/di/injection_token.ts?message=docs(core)%3A%20請簡述你的修改...#L93-L96\" aria-label=\"提供編輯建議\" title=\"提供編輯建議\"><i class=\"material-icons\" aria-hidden=\"true\" role=\"img\">mode_edit</i></a>\n  <a href=\"https://github.com/angular/angular-cn/tree/12.1.0-next.1/packages/core/src/di/injection_token.ts#L93-L96\" aria-label=\"檢視原始碼\" title=\"檢視原始碼\"><i class=\"material-icons\" aria-hidden=\"true\" role=\"img\">code</i></a>\n</div>\n    </div>\n  </th></tr></thead>\n  <tbody>\n    \n  \n    <tr>\n      <td>\n        <div class=\"overload-info\">\n  \n\n  <code-example language=\"ts\" hidecopy=\"true\" class=\"no-box api-heading\"><span class=\"member-name\">toString</span>(): string</code-example>\n\n  \n\n  <h6 class=\"no-anchor\" id=\"引數-1\">引數</h6>\n  <p>沒有引數。</p>\n\n  \n  <h6 class=\"no-anchor\" id=\"返回值\">返回值</h6>\n  <p><code>string</code></p>\n\n  \n\n\n  \n\n  \n</div>\n      </td>\n    </tr>\n  \n\n  \n\n  \n    </tbody>\n</table>\n\n  \n</section>\n\n\n\n  \n<section ng-should-translate=\"\" class=\"usage-notes\">\n  <h2 id=\"usage-notes\">使用說明<a title=\"Link to this heading\" class=\"header-link\" aria-hidden=\"true\" href=\"api/core/InjectionToken#usage-notes\"><i class=\"material-icons\">link</i></a></h2>\n  <h3 id=\"basic-example\" translation-result=\"on\">基本範例<a title=\"Link to this heading\" class=\"header-link\" aria-hidden=\"true\" href=\"api/core/InjectionToken#basic-example\"><i class=\"material-icons\">link</i></a></h3><h3 id=\"basic-example\" translation-result=\"on\">普通注入令牌<a title=\"Link to this heading\" class=\"header-link\" aria-hidden=\"true\" href=\"api/core/InjectionToken#plain-injectiontoken\"><i class=\"material-icons\">link</i></a></h3><h3 translation-origin=\"off\" id=\"basic-examplelink\">Basic Example<a title=\"Link to this heading\" class=\"header-link\" aria-hidden=\"true\" href=\"api/core/InjectionToken#basic-example\"><i class=\"material-icons\">link</i></a></h3>\n\n<h3 translation-origin=\"off\" id=\"plain-injectiontoken\">Plain InjectionToken<a title=\"Link to this heading\" class=\"header-link\" aria-hidden=\"true\" href=\"api/core/InjectionToken#plain-injectiontoken\"><i class=\"material-icons\">link</i></a></h3>\n\n<code-example path=\"core/di/ts/injector_spec.ts\" region=\"InjectionToken\">\nconst BASE_URL = new <a href=\"api/core/InjectionToken\" class=\"code-anchor\">InjectionToken</a>&lt;string&gt;('BaseUrl');\nconst injector =\n    Injector.create({providers: [{provide: BASE_URL, useValue: 'http://localhost'}]});\nconst url = injector.get(BASE_URL);\n// here `url` is inferred to be `string` because `BASE_URL` is `<a href=\"api/core/InjectionToken\" class=\"code-anchor\">InjectionToken</a>&lt;string&gt;`.\nexpect(url).toBe('http://localhost');\n\n</code-example>\n<h3 id=\"tree-shakable-injectiontoken\">Tree-shakable InjectionToken<a title=\"Link to this heading\" class=\"header-link\" aria-hidden=\"true\" href=\"api/core/InjectionToken#tree-shakable-injectiontoken\"><i class=\"material-icons\">link</i></a></h3>\n<code-example path=\"core/di/ts/injector_spec.ts\" region=\"ShakableInjectionToken\">\nclass MyService {\n  constructor(readonly myDep: MyDep) {}\n}\n\nconst MY_SERVICE_TOKEN = new <a href=\"api/core/InjectionToken\" class=\"code-anchor\">InjectionToken</a>&lt;MyService&gt;('Manually constructed MyService', {\n  providedIn: 'root',\n  factory: () =&gt; new MyService(inject(MyDep)),\n});\n\nconst instance = injector.get(MY_SERVICE_TOKEN);\nexpect(instance instanceof MyService).toBeTruthy();\nexpect(instance.myDep instanceof MyDep).toBeTruthy();\n\n</code-example>\n\n</section>\n\n\n\n  </div>\n</article>\n\n<!-- links to this doc:\n - api/common/APP_BASE_HREF\n - api/common/DOCUMENT\n - api/common/LOCATION_INITIALIZED\n - api/common/http/HTTP_INTERCEPTORS\n - api/common/testing/MOCK_PLATFORM_LOCATION_CONFIG\n - api/common/upgrade/LOCATION_UPGRADE_CONFIGURATION\n - api/core\n - api/core/ANALYZE_FOR_ENTRY_COMPONENTS\n - api/core/APP_BOOTSTRAP_LISTENER\n - api/core/APP_ID\n - api/core/APP_INITIALIZER\n - api/core/COMPILER_OPTIONS\n - api/core/ClassProvider\n - api/core/ConstructorProvider\n - api/core/DEFAULT_CURRENCY_CODE\n - api/core/ExistingProvider\n - api/core/FactoryProvider\n - api/core/INJECTOR-1\n - api/core/LOCALE_ID\n - api/core/PACKAGE_ROOT_URL\n - api/core/PLATFORM_ID\n - api/core/PLATFORM_INITIALIZER\n - api/core/ProviderToken\n - api/core/StaticClassProvider\n - api/core/TRANSLATIONS\n - api/core/TRANSLATIONS_FORMAT\n - api/core/ValueProvider\n - api/core/inject-0\n - api/core/testing/ComponentFixtureAutoDetect\n - api/core/testing/ComponentFixtureNoNgZone\n - api/forms\n - api/forms/COMPOSITION_BUFFER_MODE\n - api/forms/NG_ASYNC_VALIDATORS\n - api/forms/NG_VALIDATORS\n - api/forms/NG_VALUE_ACCESSOR\n - api/platform-browser/EVENT_MANAGER_PLUGINS\n - api/platform-browser/HAMMER_GESTURE_CONFIG\n - api/platform-browser/HAMMER_LOADER\n - api/platform-browser/animations/ANIMATION_MODULE_TYPE\n - api/platform-server/BEFORE_APP_SERIALIZED\n - api/platform-server/INITIAL_CONFIG\n - api/router/ROUTER_CONFIGURATION\n - api/router/ROUTER_INITIALIZER\n - api/router/ROUTES-1\n - api/router/upgrade/RouterUpgradeInitializer\n - api/upgrade/static/downgradeInjectable\n - errors/NG2003\n - guide/aot-compiler\n - guide/aot-metadata-errors\n - guide/dependency-injection-in-action\n - guide/dependency-injection-providers\n - guide/ngmodule-faq\n-->\n<!-- links from this doc:\n - /api\n - api/core\n - api/core/InjectionToken#_desc\n - api/core/InjectionToken#basic-example\n - api/core/InjectionToken#constructor\n - api/core/InjectionToken#constructor()\n - api/core/InjectionToken#description\n - api/core/InjectionToken#injectiontoken\n - api/core/InjectionToken#instance-methods\n - api/core/InjectionToken#instance-properties\n - api/core/InjectionToken#plain-injectiontoken\n - api/core/InjectionToken#toString\n - api/core/InjectionToken#tostring\n - api/core/InjectionToken#tree-shakable-injectiontoken\n - api/core/InjectionToken#usage-notes\n - api/core/Injector-0\n - api/core/NgModule\n - api/core/Type\n - https://github.com/angular/angular-cn/edit/aio/packages/core/src/di/injection_token.ts?message=docs(core)%3A%20請簡述你的修改...#L13-L97\n - https://github.com/angular/angular-cn/edit/aio/packages/core/src/di/injection_token.ts?message=docs(core)%3A%20請簡述你的修改...#L93-L96\n - https://github.com/angular/angular-cn/tree/12.1.0-next.1/packages/core/src/di/injection_token.ts#L13-L97\n - https://github.com/angular/angular-cn/tree/12.1.0-next.1/packages/core/src/di/injection_token.ts#L93-L96\n-->"}