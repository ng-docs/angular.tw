                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    \n      </td>\n    </tr>\n  </tbody>\n</table>\n\n  \n  <h6 class=\"no-anchor\" id=\"返回值-1\">返回值</h6>\n  <p><code><a href=\"api/core/Attribute\" class=\"code-anchor\">Attribute</a></code></p>\n\n  \n\n\n  \n\n  \n</div>\n      </td>\n    </tr>\n  \n\n  \n\n  \n    </tbody>\n</table>\n\n  \n</section>\n\n\n  \n\n\n  </div>\n</article>\n\n<!-- links to this doc:\n - api/core\n-->\n<!-- links from this doc:\n - /api\n - api/core\n - api/core/Attribute\n - api/core/AttributeDecorator#attributedecorator\n - api/core/AttributeDecorator#call\n - api/core/AttributeDecorator#call-usage-notes\n - api/core/AttributeDecorator#instance-methods\n - api/core/AttributeDecorator#new\n - api/core/AttributeDecorator#建構函式簽名\n - api/core/AttributeDecorator#呼叫簽名\n - api/core/Component\n - api/core/Directive\n - api/platform-browser/Title\n - https://github.com/angular/angular-cn/edit/aio/packages/core/src/di/metadata_attr.ts?message=docs(core)%3A%20請簡述你的修改...#L11-L50\n - https://github.com/angular/angular-cn/edit/aio/packages/core/src/di/metadata_attr.ts?message=docs(core)%3A%20請簡述你的修改...#L21-L48\n - https://github.com/angular/angular-cn/edit/aio/packages/core/src/di/metadata_attr.ts?message=docs(core)%3A%20請簡述你的修改...#L49-L49\n - https://github.com/angular/angular-cn/tree/12.1.0-next.1/packages/core/src/di/metadata_attr.ts#L11-L50\n - https://github.com/angular/angular-cn/tree/12.1.0-next.1/packages/core/src/di/metadata_attr.ts#L21-L48\n - https://github.com/angular/angular-cn/tree/12.1.0-next.1/packages/core/src/di/metadata_attr.ts#L49-L49\n-->"}